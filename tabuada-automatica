
import java.util.*;
import java.lang.*;
import java.io.*;

class Ideone
{
	public static void main (String[] args) throws java.lang.Exception
	{
		int[] numeros = new int[11];
		for(int u = 0; u < 10; u++){
			int numero = u + 1;
			System.out.println("Tabuada do numero: " + numero);
			for(int i = 0; i < numeros.length; i++){
				numeros[i] = i*numero;
				System.out.print(numeros[i] + " ");
			}
			System.out.println("");
		}
	}
}
